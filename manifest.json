{
    "manifest_version": 3,
    "name": "Custom ChatGPT",
    "version": "1.0.1",
    "description": "ChatGPTのUIをカスタマイズ、より便利に。",
    "author": "Kamada Masachika",
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "dist/bundle.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    }
}